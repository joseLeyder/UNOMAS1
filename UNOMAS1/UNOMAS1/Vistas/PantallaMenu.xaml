<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="UNOMAS1.Vistas.PantallaMenu"
             xmlns:pages="clr-namespace:UNOMAS1.Vistas">
  <MasterDetailPage.Master>
    <pages:PantallaMenuMaster x:Name="MasterPage" />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
        <NavigationPage x:Name="Navigator"
                    BarBackgroundColor="{StaticResource BarBackgroundColor}">
      <x:Arguments>
        <pages:PantallaMenuDetail />
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>